{
  "name": "@schwingbat/punch",
  "version": "2.3.0-alpha",
  "description": "A CLI time tracker that syncs.",
  "main": "./src/main.js",
  "bin": {
    "punch": "./src/main.js"
  },
  "pkg": {
    "assets": [
      "src/resources/**/*"
    ],
    "targets": [
      "node10"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "bundle": "browserify src/main.js --node --outfile=punch.js",
    "build": "npm run build:mac && npm run build:linux && npm run build:windows",
    "build:mac": "pkg package.json --out-path dist/mac --target node10-macos-x64",
    "build:windows": "pkg package.json --out-path dist/windows --target node10-win-x86",
    "build:linux": "pkg package.json --out-path dist/linux --target node10-linux-x86",
    "package": "node ./scripts/package.js"
  },
  "keywords": [],
  "author": "Tony McCoy <tony@ratwizard.io>",
  "repository": "github:schwingbat/punch",
  "license": "MIT",
  "dependencies": {
    "@fardog/wordwrap": "^1.1.0",
    "@schwingbat/is": "^2.0.0",
    "@schwingbat/mon": "^1.0.0",
    "ansi-regex": "^3.0.0",
    "aws-sdk": "^2.264.1",
    "backblaze-b2": "^1.0.4",
    "better-sqlite3": "^4.1.4",
    "chalk": "^2.4.1",
    "date-fns": "^1.29.0",
    "log-update": "^2.3.0",
    "mkdirp": "^0.5.1",
    "readline-sync": "^1.4.9",
    "time-require": "^0.1.2",
    "uuid": "^3.3.0"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "beforeEach",
      "beforeAll",
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "jest": "^23.4.0",
    "jszip": "^3.1.5"
  }
}
